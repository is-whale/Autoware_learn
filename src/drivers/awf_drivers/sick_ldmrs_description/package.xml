<?xml version="1.0"?>
<package format="2">
  <name>sick_ldmrs_description</name>
  <version>1.12.0</version>
  <description>
    This package contains an URDF description of the SICK LD-MRS scanner and all supporting mesh files.
  </description>
  <maintainer email="martin.guenther@dfki.de">Martin Günther</maintainer>
  <author email="martin.guenther@dfki.de">Martin Günther</author>
  <url type="website">http://wiki.ros.org/sick_ldmrs_description</url>
  <license>Apache 2</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roslaunch</build_depend>

  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>rviz</exec_depend>
  <exec_depend>xacro</exec_depend>

  <export>
    <architecture_independent/>
  </export>
</package>
